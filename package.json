{
  "name": "electron-with-server-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "node-ipc": "^9.1.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
